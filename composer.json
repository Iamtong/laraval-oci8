{
    "name": "iamtong/laravel-oci8",
    "description": "Oracle DB driver for Laravel 5 via OCI8",
    "keywords" : ["laravel5","laravel","oracle","oci8","pdo_oci"],
    "license": "MIT",
    "authors": [
        {
            "name": "Iamtong",
            "email": "jiantong912@163.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-oci8": ">=2.0.0",
        "illuminate/support": "5.7.*",
        "illuminate/database": "5.7.*"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "scrutinizer/ocular": "~1.1",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "files": [
          "src/helper.php"
        ],
        "psr-4": {
            "Iamtong\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
          "dev-master": "5.7-dev"
        },
        "laravel": {
            "providers": [
                "Iamtong\\Oci8\\Oci8ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
